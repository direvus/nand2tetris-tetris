class Util {
    /** Return the modulus of a % b. */
    function int mod(int a, int b) {
        var int quotient;
        let quotient = a / b;
        return a - (b * quotient);
    }

    function void printString(String str) {
        do Output.printString(str);
        do str.dispose();
        return;
    }
}
